<div class="container">
  <h1>cohorts:</h1>
  <a href="/cohorts/new" class="waves-effect waves-light btn indigo lighten-1"><i class="material-icons md-24 right">add_circle_outline</i>Add cohort</a>
  <table class="posts">
    <thead>
      <tr>
        <th>Name</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Instructor</th>
        <th>Students</th>
        <th>Show</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
    </thead>
    <% @cohorts.each do |cohort| %>
      <tbody>
        <tr class="table"></tr>
        <td> <%= cohort.name %> </td>
        <td> <%= cohort.start_date %> </td>
        <td> <%= cohort.end_date %> </td>
        <td> <%= cohort.end_date %> </td>
        <td> <%= cohort.end_date %> </td>
        <td> <a href="/cohorts/<%= cohort.id %>"><i class="material-icons md-25">pageview</i></a></td>
        <td> <a href="/cohorts/<%= cohort.id %>/edit"><i class="material-icons md-25">edit</i></a></td>
        <td> <a href="/cohorts/<%= cohort.id %>" id="delete-item" data-method="delete"><i class="material-icons md-25">delete_forever</i></a></td>
      </tr>
    </tbody>
  <% end %>
</table>
</div>
